@model Web_61131562.Models.ThongKeModel
@if (Model != null)
{
    <div class="thongke">
        <table class="table">
            <tr>
                <td>Hôm Nay</td>
                <td>@Model.HomNay</td>
            </tr>
            <tr>
                <td>Hôm Qua</td>
                <td>@Model.HomQua</td>
            </tr>
            <tr>
                <td>Tuần Này</td>
                <td>@Model.TuanNay</td>
            </tr>
            <tr>
                <td>Số Người Online</td>
                <td>@ViewBag.Visitors_online</td>
            </tr>
        </table>
    </div>
}
